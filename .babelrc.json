{
  "parserOpts": {
    "strictMode": true
  },
  "sourceMaps": "inline",
  "presets": [["@babel/preset-env", { "targets": { "node": "current" } }], "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-private-methods"
  ]
}
